import time
print("What shall I remind you?")
text = str(input())
print("In how many minutes do you want to start?")
local_time = float(input())
local_time = local_time * 60
time.sleep(local_time)
print(text)
local_time = [] #blank list
temp = input()
local_time.append(temp) #add to list
print (local_time)
